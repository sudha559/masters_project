  <servlet>
    <servlet-name>patientController</servlet-name>
    <servlet-class>omss.controllers.PatientController</servlet-class>
  </servlet>
  
    <servlet>
    <servlet-name>appointmentController</servlet-name>
    <servlet-class>omss.controllers.AppointmentController</servlet-class>
  </servlet>
  

  <servlet>
    <servlet-name>pdfController</servlet-name>
    <servlet-class>omss.controllers.PdfController</servlet-class>
  </servlet>
  
     <servlet>
