     <servlet>
     	<servlet-name>logoffController</servlet-name>
     	<servlet-class>omss.controllers.LogoffController</servlet-class>
     </servlet>
     
     
     <servlet>
     	<servlet-name>contactUsController</servlet-name>
     	<servlet-class>omss.controllers.ContactUsController</servlet-class>
     </servlet>
     
     <servlet>
     	<servlet-name>aboutUsController</servlet-name>
