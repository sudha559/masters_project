@import url(http://fonts.googleapis.com/css?family=Montserrat:400,700);
#omss_menu,
#omss_menu ul,
#omss_menu ul li,
#omss_menu ul li a,
#omss_menu #omss_menu-button {
  margin: 0;
  padding: 0;
  border: 0;
  list-style: none;
  line-height: 1;
  display: block;
  position: relative;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
#omss_menu:after,
#omss_menu > ul:after {
  content: ".";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}
#omss_menu #omss_menu-button {
  display: none;
}
#omss_menu {
	background: #333333;
  font-family: Montserrat, sans-serif;
  
}
#omss_menu > ul > li {
  float: left;
}
#omss_menu.align-center > ul {
  font-size: 0;
  text-align: center;
}
/* #omss_menu.align-center > ul > li {
  display: inline-block;
  float: none;
} */
/* #omss_menu.align-center ul ul {
  text-align: left;
} */
#omss_menu.align-right > ul > li {
  float: right;
}
#omss_menu > ul > li > a {
  padding: 17px;
  color: #dddddd;
  //font-weight: 800;
  font-size: 12px;
  letter-spacing: 1px;
  text-decoration: none;
  text-transform: uppercase;
  
}

#omss_menu > ul > li:hover > a {
  color: #ffffff;
}
#omss_menu > ul > li.has-sub > a {
  padding-right: 30px;
}
#omss_menu > ul > li.has-sub > a:after {
  position: absolute;
  top: 22px;
  right: 11px;
  width: 8px;
  height: 2px;
  display: block;
  background: #dddddd;
  content: '';
}
#omss_menu > ul > li.has-sub > a:before {
  position: absolute;
  top: 19px;
  right: 14px;
  display: block;
  width: 2px;
  height: 8px;
  background: #dddddd;
  content: '';
  -webkit-transition: all .25s ease;
  -moz-transition: all .25s ease;
  -ms-transition: all .25s ease;
  -o-transition: all .25s ease;
  transition: all .25s ease;
}
#omss_menu > ul > li.has-sub:hover > a:before {
  top: 23px;
  height: 0;
}
#omss_menu ul ul {
  position: absolute;
  left: -9999px;
}
#omss_menu.align-right ul ul {
  text-align: right;
}
#omss_menu ul ul li {
  height: 0;
  -webkit-transition: all .25s ease;
  -moz-transition: all .25s ease;
  -ms-transition: all .25s ease;
  -o-transition: all .25s ease;
  transition: all .25s ease;
}
#omss_menu li:hover > ul {
  left: auto;
}
#omss_menu.align-right li:hover > ul {
  left: auto;
  right: 0;
}
#omss_menu li:hover > ul > li {
  height: 35px;
}
#omss_menu ul ul ul {
  margin-left: 100%;
  top: 0;
}
#omss_menu.align-right ul ul ul {
  margin-left: 0;
  margin-right: 100%;
}
#omss_menu ul ul li a {
  border-bottom: 1px solid rgba(150, 150, 150, 0.15);
  padding: 11px 15px;
  width: 170px;
  font-size: 12px;
  font-weight: 800;
  text-decoration: none;
  color: #dddddd;
  background: #333333;
}
#omss_menu ul ul li:last-child > a,
#omss_menu ul ul li.last-item > a {
  border-bottom: 0;
}
#omss_menu ul ul li:hover > a,
#omss_menu ul ul li a:hover {
  color: #ffffff;
}
#omss_menu ul ul li.has-sub > a:after {
  position: absolute;
  top: 16px;
  right: 11px;
  width: 8px;
  height: 2px;
  display: block;
  //background: #dddddd;
  background: white;
  content: '';
}
#omss_menu.align-right ul ul li.has-sub > a:after {
  right: auto;
  left: 11px;
}
#omss_menu ul ul li.has-sub > a:before {
  position: absolute;
  top: 13px;
  right: 14px;
  display: block;
  width: 2px;
  height: 8px;
  background: #dddddd;
  content: '';
  -webkit-transition: all .25s ease;
  -moz-transition: all .25s ease;
  -ms-transition: all .25s ease;
  -o-transition: all .25s ease;
  transition: all .25s ease;
}
#omss_menu.align-right ul ul li.has-sub > a:before {
  right: auto;
  left: 14px;
}
#omss_menu ul ul > li.has-sub:hover > a:before {
  top: 17px;
  height: 0;
}
