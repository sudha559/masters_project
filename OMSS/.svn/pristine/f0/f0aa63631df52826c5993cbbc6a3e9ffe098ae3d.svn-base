  }
  #omss_menu ul ul li,
  #omss_menu li:hover > ul > li {
    height: auto;
  }
  #omss_menu ul li a,
  #omss_menu ul ul li a {
    width: 100%;
    border-bottom: 0;
  }
  #omss_menu > ul > li {
    float: none;
  }
  #omss_menu ul ul li a {
    padding-left: 25px;
  }
  #omss_menu ul ul ul li a {
    padding-left: 35px;
  }
  #omss_menu ul ul li a {
    color: #dddddd;
    background: none;
  }
  #omss_menu ul ul li:hover > a,
  #omss_menu ul ul li.active > a {
    color: #ffffff;
  }
  #omss_menu ul ul,
  #omss_menu ul ul ul,
  #omss_menu.align-right ul ul {
    position: relative;
    left: 0;
    width: 100%;
    margin: 0;
    text-align: left;
  }
  #omss_menu > ul > li.has-sub > a:after,
  #omss_menu > ul > li.has-sub > a:before,
  #omss_menu ul ul > li.has-sub > a:after,
  #omss_menu ul ul > li.has-sub > a:before {
    display: none;
  }
  #omss_menu #omss_menu-button {
    display: block;
    padding: 17px;
    color: #dddddd;
    cursor: pointer;
    font-size: 12px;
    text-transform: uppercase;
    font-weight: 700;
  }
  #omss_menu #omss_menu-button:before {
